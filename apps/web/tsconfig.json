{
  "include": ["**/*", ".react-router/types/**/*"],
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "rootDirs": [".", "./.react-router/types"],
    "types": ["node", "vite/client"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    }
  }
}
